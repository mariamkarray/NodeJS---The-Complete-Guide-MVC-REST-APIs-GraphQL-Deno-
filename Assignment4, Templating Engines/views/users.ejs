<%- include('includes/head.ejs') %> 
    <link rel="stylesheet" href="/css/users.css">
</head>
<body>
    <%- include('includes/navigation.ejs') %> 
<main>
    <% if (users.length){ %>
    <div class="grid">
        <% for (let user of users) {%>
            <ul>
                <li> <%= user.user %> </li>
              </ul>
        <% } %> 
    </div>
<% } else {%>
    <h1> No Users Found </h1>
<% } %> 

</main>

<%- include('includes/end.ejs') %> 